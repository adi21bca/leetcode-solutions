class Main {
    public static void main(String[] args) {
      int arr[] = {1,2,3};
      int brr[] = new int[arr.length * 2];
      int j = 0 ;
      for(int i= 0 ;i<brr.length;i++){
           if( j == arr.length){
              j = 0;
           }
           brr[i] = arr[j];
               j++;
      }
      System.out.println(Arrays.toString(brr));
  }
}
