import java.util.Arrays;
class Main {
    public static void main(String[] args) {
       int[] nums = {0,2,1,5,3,4};
       int[] num = new int[nums.length];
       for(int i = 0 ; i < nums.length ; i++){
           num[i] = nums[nums[i]];
       }
       System.out.println(Arrays.toString(num));
    }
}
